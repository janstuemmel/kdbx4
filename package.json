{
  "name": "kdbx4",
  "type": "module",
  "version": "0.1.0",
  "description": "A browser-only kdbx4 parser",
  "exports": {
    "./*.js": {
      "types": "./types/*.d.ts",
      "default": "./lib/*.js"
    }
  },
  "scripts": {
    "clean": "rm -rf types *tgz",
    "test": "vitest --reporter=verbose",
    "build": "npm run clean && tsc -p tsconfig.build.json",
    "lint": "xo",
    "type-check": "tsc"
  },
  "xo": {
    "space": true,
    "rules": {
      "capitalized-comments": "off",
      "unicorn/prefer-code-point": "off"
    }
  },
  "keywords": [
    "keepass",
    "kdbx4",
    "kdbx"
  ],
  "license": "MIT",
  "devDependencies": {
    "typescript": "^5.2.2",
    "vitest": "^0.34.6",
    "xo": "^0.56.0"
  },
  "volta": {
    "node": "20.9.0",
    "npm": "10.2.1"
  }
}
