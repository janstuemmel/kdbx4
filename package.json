{
  "name": "kdbx4",
  "type": "module",
  "version": "0.1.0",
  "description": "A browser-only kdbx4 parser",
  "exports": {
    "./*.js": {
      "types": "./types/*.d.ts",
      "default": "./lib/*.js"
    }
  },
  "scripts": {
    "clean": "rm -rf types",
    "test": "vitest",
    "build": "npm run clean && tsc -p tsconfig.build.json",
    "lint": "xo .",
    "type-check": "tsc"
  },
  "xo": {
		"space": true
	},
  "keywords": [
    "keepass",
    "kdbx4",
    "kdbx"
  ],
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.8.7",
    "@vitest/browser": "^0.34.6",
    "playwright": "^1.39.0",
    "typescript": "^5.2.2",
    "vitest": "^0.34.6",
    "xo": "^0.56.0"
  }
}
